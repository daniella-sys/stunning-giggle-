const числа = [1, 2, 3, 4, 5];
const numbersIS = числа.filter(isNumber);
console.log(numbersIS);
// Отримати тільки парні числа
// Очікується: [2, 4]
function isNumber(element){
  return element % 2 === 0;
};

function Notnumber(element){
  return element % 2 !== 0;
};
const NotnumberIS = числа.filter(Notnumber);  // Змінна number не існує, має бути числа
console.log(Notnumber);

const числа1 = [10, 25, 30, 45, 50];
// Отримати числа менші за 40
// Очікується: [10, 25, 30]
function isNUUmver(element){
  return element >= 40;
};
const числа_менш40 = числа1.filter(isNUUmver);
console.log(числа_менш40);

function числаблш40(element){
  return element < 40;
};
const числаблш408 = числа1.filter(числаблш40);
console.log(числаблш408);

const числа2 = [3, 7, 12, 8, 15, 20];
// Отримати числа, які діляться на 4
// Очікується: [12, 8, 20]

// Підказка: перевірка подільності: число % 4 === 0
// Твій код тут:
function числа_що_дл(element){
  return element % 4 === 0;
};
const числадлт = числа2.filter(числа_що_дл);
console.log(числадлт);

function числаnot(element){
  return element % 4 !== 0;
};
const числа_що_не_длт = числа2.filter(числаnot);
console.log(числа_що_не_длт);

const слова = ['мама', 'тато', 'бабуся', 'дідусь', 'син'];

// Завдання: отримати слова, що починаються на 'б'
// Очікується: ['бабуся']
// Підказка: слово.startsWith('б')

// Твій код тут (називай змінні як хочеш!):
function слова_що_почин(element){
  return element.startWith("б");
};
const слова_що_почин5 = слова.filter(слова_що_почин);
console.log(слова_що_почин5);
function словаNOt(element){
  return !element.startWith("б");
};
const слова_щоNot = слова.filter(словаNOt);
console.log(слова_щоNot);

const температури = [20, 25, 18, 30, 22, 19];
// Отримати температури в комфортному діапазоні (20-25°C включно)
// Очікується: [20, 25, 22]
// Підказка: температура >= 20 && температура <= 25
function comforttemp(element){
  return element >= 20 && температури <= 25;
};
const сomfort = температури.filter(comforttemp);
console.log(сomfort);

function comfortNOT(element){
return !element.температури  >= 20 && температури <= 25;
};
const температураNOt = температури.filter(comfortNOT);
console.log(температураNOt);

const студенти = [
  { імʼя: 'Анна', вік: 20 },
  { імʼя: 'Петро', вік: 17 },
  { імʼя: 'Марія', вік: 22 }
];

// Завдання: отримати повнолітніх студентів (вік >= 18)
// Очікується: [{імʼя: 'Анна', вік: 20}, {імʼя: 'Марія', вік: 22}]
// Тут вже потрібно студент.вік!

// Твій код тут:
function adults(element){
  return element.вік >= 18;
};
const isStudent = студенти.filter(adults);
console.log(isStudent);

function Notadults(element){
  return element.вік < 18;     //порівнюєш весь об'єкт з числом 18, а потрібно порівнювати властивість об'єкта element.вік
};
const NOTadualys = студенти.filter(Notadults);
console.log(NOTadualys);


const товари = [
  { назва: 'Телефон', ціна: 10000, вНаявності: true },
  { назва: 'Ноутбук', ціна: 30000, вНаявності: false },
  { назва: 'Навушники', ціна: 1500, вНаявності: true }
];

// Завдання 1: отримати товари в наявності
// Завдання 2: отримати товари дешевші за 20000

// Твій код тут:
function товарВнаявності(element){
  return element.вНаявності;
};
const наявність = товари.filter(товарВнаявності);
console.log(наявність);
function дешевітовари(element){
  return element.ціна < 20000;
};
const hjgfthfyu = товари.filter(дешевітовари);
console.log(hjgfthfyu);

const товари6 = [
  { назва: 'Телефон', ціна: 10000, вНаявності: true },
  { назва: 'Ноутбук', ціна: 30000, вНаявності: false },
  { назва: 'Навушники', ціна: 1500, вНаявності: true },
  { назва: 'Планшет', ціна: 15000, вНаявності: true },
  { назва: 'Монітор', ціна: 5000, вНаявності: false }
];

// Завдання: отримати товари, які:
// 1) Є в наявності (вНаявності: true)
// 2) Коштують менше 15000 грн

// Очікується: [{телефон...}, {навушники...}]
// Підказка: element.вНаявності && element.ціна < 15000

// Твій код тут:
function vnavnosti(element){
  return element.вНаявності;
};
const vnavnosti2 = товари6.filter(vnavnosti);
console.log(vnavnosti2);

function menshe(element){
  return element.ціна < 15000;
};
const mensh = товари6.filter(menshe);
console.log(mensh);

//HSRD
const студенти778 = [
  { імʼя: 'Анна', вік: 20, оцінка: 85, активний: true },
  { імʼя: 'Петро', вік: 22, оцінка: 92, активний: false },
  { імʼя: 'Марія', вік: 19, оцінка: 78, активний: true },
  { імʼя: 'Олег', вік: 21, оцінка: 88, активний: true },
  { імʼя: 'Наталія', вік: 23, оцінка: 95, активний: false }
];
// Отримати студентів, які:       ///function відбірСтудентів(element) {
//  return element.активний &&           // 1) Активний
//         element.вік >= 20 &&          // 2) Вік від 20
 //        element.вік <= 22 &&          //    до 22 включно    АБО ТАК ЗАПИСАТИ
 //        element.оцінка > 80;          // 3) Оцінка > 80
 //}

function activ(element){
  return element.активний;
};
const activ1 = студенти778.filter(activ);
console.log(activ1);
function age(element){
  return element.вік >= 19 && element.вік < 22;
};
const agt1 = студенти778.filter(age);
console.log(agt1);
function ocinka(element){
  return element.оцінка > 80;
};
const ocinka1 = студенти778.filter(ocinka);
console.log(ocinka1);

const продукти = [
  { назва: 'Яблуко зелене', категорія: 'фрукти', ціна: 30 },
  { назва: 'Яблуко червоне', категорія: 'фрукти', ціна: 35 },
  { назва: 'Банан', категорія: 'фрукти', ціна: 40 },
  { назва: 'Яблучний сік', категорія: 'напої', ціна: 50 },
  { назва: 'Яблучний пиріг', категорія: 'випічка', ціна: 100 }
];
// Отримати ВСІ продукти, назва яких містить 'Яблук'
// (незалежно від того, що йде після)
// Очікується: [{Яблуко зелене...}, {Яблуко червоне...}, {Яблучний сік...}, {Яблучний пиріг...}]
// Підказка: назва.includes('Яблук')
function bhgy(element){
  return element.назва.includes('Яблук');
};
const name676 = продукти.filter(bhgy);
console.log(name676);

const продуктиg = [
  { назва: 'Яблуко зелене', категорія: 'фрукти', ціна: 30 },
  { назва: 'Яблуко червоне', категорія: 'фрукти', ціна: 35 },
  { назва: 'Банан', категорія: 'фрукти', ціна: 40 },
  { назва: 'Яблучний сік', категорія: 'напої', ціна: 50 },
  { назва: 'Яблучний пиріг', категорія: 'випічка', ціна: 100 },
  { назва: 'Груша', категорія: 'фрукти', ціна: 25 }
];

// Завдання: отримати продукти, які:
// 1) Містять 'Яблук' в назві
// 2) Належать до категорії 'фрукти'
// Очікується: [{Яблуко зелене...}, {Яблуко червоне...}]

// Твій код тут:
function hhuj(element){
  return element.назва.includes('Яблук') && element.категорія === 'фрукти';
};
const ghh = продуктиg.filter(hhuj);
console.log(ghh);
