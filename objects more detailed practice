
const book4 = {
 title: "",
 author: "",
 year: ""
}
console.log(book4.title);
console.log(book4.author);
console.log(book4.year);
book4.year = 2024; //змінила рік
console.log(book4.year); //вивела новий рік

const library = {
    books: [
        { title: "назва1", author: "автор1", year: 2000 },
        { title: "назва2", author: "автор2", year: 2005 }
    ]
};

console.log(library.books[0].title);
console.log(library.books[0].author);
console.log(library.books[0].year);
console.log(library.books[1].title);
console.log(library.books[0].author);
console.log(library.books[1].year);
//додавання третьої книги

library.books.push({ title: "назва3", author: "автор3", year: "2000"})

Вивести: "Всього книг: " + результат
console.log(library.books);
//Порахувати скільки книг в масиві
const lll = (library.books.length);
console.log(lll);


